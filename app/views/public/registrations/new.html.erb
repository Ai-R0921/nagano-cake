<div class="container">
  <h3>新規会員登録</h3>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= render "public/shared/error_messages", resource: resource %>
      <div class="row mt-5 mb-3">
        <%= f.label :last_name, "名前", class:"col-2" %>
        <%= f.label :last_name, "(姓)", class:"col-1 text-right" %>
        <%= f.text_field :last_name, :placeholder => "令和", class:"col-3" %>
        <%= f.label :first_name, "(名)", class:"col-1 text-right" %>
        <%= f.text_field :first_name, :placeholder => "道子", class:"col-3" %>
      </div>
      <div class="row mb-3">
        <%= f.label :last_name_kana, "フリガナ", class:"col-2" %>
        <%= f.label :last_name_kana, "(セイ)", class:"col-1 text-right" %>
        <%= f.text_field :last_name_kana, :placeholder => "レイワ", class:"col-3" %>
        <%= f.label :first_name_kana, "(メイ)", class:"col-1 text-right" %>
        <%= f.text_field :first_name_kana, :placeholder => "ミチコ", class:"col-3" %>
      </div>
      <% end %>
</div>